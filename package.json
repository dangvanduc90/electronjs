{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "publish": "build -p always"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron-builder": "^20.28.4",
    "electron": "^3.0.0",
    "electron-log": "^1.3.0",
    "electron-updater": "^3.1.2"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "electron-windows-badge": "^1.0.2",
    "moment": "^2.22.2",
    "node-fetch": "^2.2.0",
    "node-notifier": "^5.2.1",
    "nodemon": "^1.18.4"
  },
  "build": {
    "appId": "com.github.iffy.electronupdaterexample",
    "publish": [
      {
        "provider": "github",
        "owner": "dangvanduc90",
        "repo": "electron-updater-example"
      }
    ],
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    }
  }
